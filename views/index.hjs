<html>

    <head>
        <link
            href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
            crossorigin="anonymous">
        <link rel="stylesheet" type="text/css"
            href="/css/bootstrap/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="/css/style.css">
        <script type="module" src="js/menuEvents.js"></script>
    </head>

    <body>
        <div class="container-fluid">

            <div id="Opening" class="jumbotron">

                <h2 style="border-top-left-radius: 0;
                    border-top-right-radius: 0; padding: 60px;" class="display-4">Scott Cooper
                    <br>
                    {{#paragraphData}}
                    <!-- Web dev title -->
                    {{#websiteProjectsEnabled}}
                    <a href="#websites">{{webDevTitle}}</a>
                    {{/websiteProjectsEnabled}}
                    <!-- Slash if showing both -->
                    {{#bothProjectTypesEnabled}}
                    /
                    {{/bothProjectTypesEnabled}}
                    <!-- Game dev title -->
                    {{#gameProjectsEnabled}}<a
                        href="#games">{{gameDevTitle}}</a>
                    {{/gameProjectsEnabled}}


                    {{/paragraphData}}
                </h2>
                {{#paragraphData}}
                <p class="lead">{{introParagraph1}}</p>


                <p class="lead">{{introParagraph2}}</p>
                {{/paragraphData}}
            </div>

            {{#websiteProjectsEnabled}}
            <div id="websites_Scroll">
            </div>

            <div class="BigSpace">
            </div>




            <!-- Website Menu -->


            <div id="websites" style="border-radius: 0px;">


                <h2 style="border-radius: 0px;">Website Projects</h2>
                {{#paragraphData}}
                <p style="text-align: center;" class="lead">{{webDevParagraph}}</p>
                {{/paragraphData}}

                <!-- Big projects featured first -->
                <div class="container">
                    <div class="row d-flex justify-content-center" style="width:
                        100%">
                        {{#websiteData}}
                        {{#isGame}}
                        {{/isGame}}
                        {{^isGame}}
                        {{#big}}
                        <div class="col-sm-3">

                            <div class="divider">
                                <!-- Show either thumbnail image or first regular image -->
                                {{#thumbnail_string}}
                                <img src={{thumbnail_string}} class="bigImg"
                                    style="display: block;">
                                {{/thumbnail_string}}
                                {{^thumbnail_string}}
                                <img src={{image0_string}} class="bigImg"
                                    style="display: block;">
                                {{/thumbnail_string}}

                                <button class="bigBtn" value={{id}}>{{name}}</button>
                            </div>
                        </div>
                        {{/big}}
                        {{/isGame}}
                        {{/websiteData}}
                    </div>
                </div>
                <br>

                <div class="row d-flex justify-content-center" style="width:
                        100%">

                           <div class="col-sm-1"> </div>



                        {{#websiteData}}
                        {{#isGame}}
                        {{/isGame}}
                        {{^isGame}}
                        {{#big}}
                        {{/big}}
                        {{^big}}
                                <div class="col-sm-2">
                                    <div class="divider">
                                                                    {{#thumbnail_string}}
                                <img src={{thumbnail_string}}
                                    style="display: block;">
                                {{/thumbnail_string}}
                                {{^thumbnail_string}}
                                <img src={{image0_string}}
                                    style="display: block;">
                                {{/thumbnail_string}}
                                
                                    <button class="btn" value={{id}}>{{name}}</button>
                                    </div>
                                </div>

                        {{/big}}
                        {{/isGame}}
                        {{/websiteData}}

                </div>
                            <div style="padding-bottom: 64px;"></div>

            </div>
                        <div class="BigSpace">
            </div>
            <!-- Website Skills -->
            <div class="container" id="webDevSkills">
                <div class ="row" style="width: 100%; background: none;">
                        <div class ="col-sm">
                            
                            <h2 class="display-4">Web Developer Skills </h2>
                        </div>
                        <div class ="col-lg">
                                {{#paragraphData}}
                                <ul class="list-group">
                                    {{#webDevSkills}}
                                    
                                        <li class="list-group-item
                                            list-group-item-dark
                                            colour">{{{name}}}</li>


                                    {{/webDevSkills}}
                                </ul>
                                {{/paragraphData}}

                        </div>
                        
                    </div>
                </div>
            </div>


            <br>



            <div class="BigSpace">
            </div>

            {{/websiteProjectsEnabled}}

            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}


            <div id="games_Scroll">


                {{#gameProjectsEnabled}}
                <!-- Game Menu -->

                <div id="games">
                    <h2>Game Projects</h2>
                    {{#paragraphData}}
                    <p style="text-align: center;" class="lead">{{gameDevParagraph}}</p>
                    {{/paragraphData}}

                    <!-- Big projects featured first -->
                    <div class="container">
                        <div class="row d-flex justify-content-center"
                            style="width: 100%">
                            {{#websiteData}}
                            {{#isGame}}
                            {{#big}}
                            <div class="col-sm-3">

                                <div class="divider">
                                        {{#thumbnail_string}}
                                        <img src={{thumbnail_string}} class="bigImg"
                                            style="display: block;">
                                        {{/thumbnail_string}}
                                        {{^thumbnail_string}}
                                        <img src={{image0_string}} class="bigImg"
                                            style="display: block;">
                                        {{/thumbnail_string}}
                                    <button class="bigBtn" value={{id}}>{{name}}</button>
                                </div>
                            </div>
                            {{/big}}
                            {{/isGame}}
                            {{^isGame}}

                            {{/isGame}}
                            {{/websiteData}}
                        </div>
                    </div>
                    <br>


                        <div class="row d-flex justify-content-center"
                            style="width: 100%">

                            {{#websiteData}}
                            {{#isGame}}
                            {{#big}}
                            {{/big}}
                            {{^big}}
                            <div class="col-sm-2">
                                <div class="divider">
                                        {{#thumbnail_string}}
                                        <img src={{thumbnail_string}} class="bigImg"
                                            style="display: block;">
                                        {{/thumbnail_string}}
                                        {{^thumbnail_string}}
                                        <img src={{image0_string}} class="bigImg"
                                            style="display: block;">
                                        {{/thumbnail_string}}
                                    <button class="btn" value={{id}}>{{name}}</button>
                                </div>
                            </div>
                            {{/big}}
                            {{/isGame}}
                            {{/websiteData}}
                        </div>

                </div>

            <div class="BigSpace"></div>

            <!--Game Dev Skills -->
            <div class="container" id="gameDevSkills">
                <div class ="row" style="width: 100%; background: none;">
                        <div class ="col-sm">
                            
                            <h2 class="display-4">Game Programmer Skills </h2>
                        </div>
                        <div class ="col-lg">
                                {{#paragraphData}}
                                <ul class="list-group">
                                    {{#gameDevSkills}}
                                    
                                        <li class="list-group-item
                                            list-group-item-dark
                                            colour">{{{name}}}</li>


                                    {{/gameDevSkills}}
                                </ul>
                                {{/paragraphData}}

                        </div>
                        
                    </div>
                </div>

                {{/gameProjectsEnabled}}



                <!-- Project Information, Images and Links -->

                {{#websiteData}}
                <div id={{id}} style="display: none;">


                    <!--Show row of images, videos and a description at the end. !-->
                    <div class="project">

                        <div class="container">

                            <div class="row"
                                style="border-bottom-left-radius: 0;
                                border-bottom-right-radius: 0;">

                                <h2 style="padding: 1px; padding-top:0px;
                                margin-bottom:none; width:100%;">{{name}}
                                <button class="close" value={{id}}>X</button>
                                </h2>


                                <div class="col-lg">
                                    <div class="container">

                                        <div class="col-md">

                                            {{#video_string}}

                                            <div class="divider">
                                                <iframe width="320" height="256"
                                                    src={{video_string}}
                                                    frameborder="0"
                                                    allow="accelerometer;
                                                    autoplay; encrypted-media;
                                                    gyroscope;
                                                    picture-in-picture"
                                                    allowfullscreen></iframe>
                                            </div>

                                            {{/video_string}}

                                            <div class="divider">
                                                <img src={{image0_string}}
                                                    class="previewImage">
                                            </div>

                                        </div>


                                        <br>
                                        <br>
                                        <div class="col" style="text-align:
                                            center;">
                                            <!--<button class="prevPreview"
                                                id={{id}}_prev>
                                            </button> -->
                                            <span>
                                                <button class="changePreview"
                                                    value=0 id={{id}}_preview>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=1>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=2>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=3> <br><br><br>
                                                </button>
                                            </span>
                                            <!--<button class="nextPreview"
                                                id={{id}}_next>
                                            </button> -->
                                        </div>
                                        <br>
                                        <br>


                                    </div>
                                </div>

                                <div class="col-xl"> <!--Description and links-->
                                    <div class="container">

                                        <!--Description -->
                                        <div class="col-lg">
                                            <br>
                                            {{description0}}
                                            <br>
                                            <br>
                                            {{description1}}
                                            <br>
                                            <br>
                                        </div>
                                    </div>



                                </div>
                            </div>
                            <div class="row" style="border-top-left-radius: 0;
                                border-top-right-radius: 0;">


                                <!-- Framework icons -->
                                 
                                <div class="col-sm-2">

                                    <img src={{frameworkImage_string}}
                                        
                                        class="framework">

                                </div>

                                
                                <div class="col-sm-1"></div>


                                <div class="col-sm-2">
                                    {{#frameworkImage1_string}}

                                    <img src={{frameworkImage1_string}}
                                        
                                        class="framework">
                                    {{/frameworkImage1_string}}

                                </div>

                                <!--Link to website and/or repository -->
                                <div class="col-lg" style="text-align:center;">
                                    {{#url}}
                                    <br>
                                    <br>
                                    <a href={{url}} class="siteLink"
                                        target="_blank"> Visit Site</a>
                                    <br>
                                    <br>
                                    {{/url}}
                                    {{#repoLink}}
                                    <a href={{repoLink}}
                                        class="repoLink"
                                        target="_blank"> GitHub <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-github" viewBox="0 0 16 16">
                                    <path d="M8 0C3.58 0 0 3.58 0 8c0 3.54 2.29 6.53 5.47 7.59.4.07.55-.17.55-.38 0-.19-.01-.82-.01-1.49-2.01.37-2.53-.49-2.69-.94-.09-.23-.48-.94-.82-1.13-.28-.15-.68-.52-.01-.53.63-.01 1.08.58 1.23.82.72 1.21 1.87.87 2.33.66.07-.52.28-.87.51-1.07-1.78-.2-3.64-.89-3.64-3.95 0-.87.31-1.59.82-2.15-.08-.2-.36-1.02.08-2.12 0 0 .67-.21 2.2.82.64-.18 1.32-.27 2-.27.68 0 1.36.09 2 .27 1.53-1.04 2.2-.82 2.2-.82.44 1.1.16 1.92.08 2.12.51.56.82 1.27.82 2.15 0 3.07-1.87 3.75-3.65 3.95.29.25.54.73.54 1.48 0 1.07-.01 1.93-.01 2.2 0 .21.15.46.55.38A8.012 8.012 0 0 0 16 8c0-4.42-3.58-8-8-8z"/></svg>

                                    </a>
                                    {{/repoLink}}
                                    <br>
                                    <br>
                                </div>

                            </div>
                        </div>

                        <div class="itemStrings" style="display: none;">
                            <p>{{video_string}}</p>
                            <p>{{image0_string}}</p>
                            <p>{{image1_string}}</p>
                            <p>{{image2_string}}</p>
                            <p>{{image3_string}}</p>
                        </div>
                    </div>
                </div>
                {{/websiteData}}

            </div>


            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}

            <a id="contactMe"
                href="https://mail.google.com/mail/u/0/?view=cm&fs=1&tf=1&source=mailto&su=&to=ScottCooperDeveloper@gmail.com"
                style="content-align: center;">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi
                    bi-envelope-fill" fill="currentColor"
                    xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M.05 3.555A2 2 0 0 1 2 2h12a2 2
                        0 0 1 1.95 1.555L8 8.414.05 3.555zM0
                        4.697v7.104l5.803-3.558L0 4.697zM6.761 8.83l-6.57
                        4.027A2 2 0 0 0 2 14h12a2 2 0 0 0
                        1.808-1.144l-6.57-4.027L8
                        9.586l-1.239-.757zm3.436-.586L16 11.801V4.697l-5.803
                        3.546z" />
                    </svg> Contact Me</a>
            <div style="padding-bottom: 32px;"></div>
            </body>

        </html>