<html>

    <head>
        <link
            href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
            crossorigin="anonymous">
        <link rel="stylesheet" type="text/css"
            href="/css/bootstrap/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="/css/style.css">
        <script type="module" src="js/menuEvents.js"></script>
        <script
            src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script
            src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
        <script
            src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    </head>

    <body>
        <div class="container-fluid">

            <div id="Opening" class="jumbotron">

                <h2 style="border-top-left-radius: 0;
                    border-top-right-radius: 0;" class="display-4">Scott Cooper
                    <br>
                    {{#paragraphData}}
                    <!-- Web dev title -->
                    {{#websiteProjectsEnabled}}
                    <a href="#websites">{{webDevTitle}}</a>
                    {{/websiteProjectsEnabled}}
                    <!-- Slash if showing both -->
                    {{#bothProjectTypesEnabled}}
                    /
                    {{/bothProjectTypesEnabled}}
                    <!-- Game dev title -->
                    {{#gameProjectsEnabled}}<a
                        href="#games">{{gameDevTitle}}</a>
                    {{/gameProjectsEnabled}}


                    {{/paragraphData}}
                </h2>
                {{#paragraphData}}
                <p class="lead">{{introParagraph1}}</p>


                <p class="lead">{{introParagraph2}}</p>
                {{/paragraphData}}
            </div>

            {{#websiteProjectsEnabled}}
            <div id="websites_Scroll">
            </div>

            <div class="BigSpace">
            </div>




            <!-- Website Menu -->


            <div id="websites">


                <h2>Website Projects</h2>
                {{#paragraphData}}
                <p style="text-align: center;" class="lead">{{webDevParagraph}}</p>
                {{/paragraphData}}

                <!-- Big projects featured first -->
                <div class="container">
                    <div class="row d-flex justify-content-center" style="width:
                        100%">
                        {{#websiteData}}
                        {{#isGame}}
                        {{/isGame}}
                        {{^isGame}}
                        {{#big}}
                        <div class="col-sm-3">

                            <div class="divider">
                                <img src={{image0_string}} class="bigImg"
                                    style="display: block;">
                                <button class="bigBtn" value={{id}}>{{name}}</button>
                            </div>
                        </div>
                        {{/big}}
                        {{/isGame}}
                        {{/websiteData}}
                    </div>
                </div>
                <br>

                <div class="row d-flex justify-content-center" style="width:
                        100%">

                           <div class="col-sm-1"> </div>



                        {{#websiteData}}
                        {{#isGame}}
                        {{/isGame}}
                        {{^isGame}}
                        {{#big}}
                        {{/big}}
                        {{^big}}
                                <div class="col-sm-2">
                                    <div class="divider">
                                    <img src={{image0_string}} style="width: 128px;
                                        height: 128px; display: block;">
                                    <button class="btn" value={{id}}>{{name}}</button>
                                    </div>
                                </div>

                        {{/big}}
                        {{/isGame}}
                        {{/websiteData}}

                </div>
            </div>

            <!-- Website Skills -->
            <div id="webDevSkills" class="main">
                {{#paragraphData}}
                <ul class="list-group">
                    <li class="list-group-item list-group-item-dark
                        list-group-item-action colour blue"
                        data-toggle="collapse"
                        data-target="#webDevSkillList"><b>Web
                            Developer Skills</b></li>
                    {{#webDevSkills}}
                    <div id="webDevSkillList" class="collapse">

                        <li class="list-group-item
                            list-group-item-dark
                            colour">{{{name}}}</li>
                    </div>

                    {{/webDevSkills}}
                </ul>
                {{/paragraphData}}

            </div>

            <br>



            <div class="BigSpace">
            </div>

            {{/websiteProjectsEnabled}}

            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}


            <div id="games_Scroll">


                {{#gameProjectsEnabled}}
                <!-- Game Menu -->

                <div id="games">
                    <h2>Game Projects</h2>
                    {{#paragraphData}}
                    <p style="text-align: center;" class="lead">{{gameDevParagraph}}</p>
                    {{/paragraphData}}

                    <!-- Big projects featured first -->
                    <div class="container">
                        <div class="row d-flex justify-content-center"
                            style="width: 100%">
                            {{#websiteData}}
                            {{#isGame}}
                            {{#big}}
                            <div class="col-sm-3">

                                <div class="divider">
                                    <img src={{image0_string}} style="width:
                                        256px;
                                        height: 256px; display: block;">
                                    <button class="bigBtn" value={{id}}>{{name}}</button>
                                </div>
                            </div>
                            {{/big}}
                            {{/isGame}}
                            {{^isGame}}

                            {{/isGame}}
                            {{/websiteData}}
                        </div>
                    </div>
                    <br>


                        <div class="row d-flex justify-content-center"
                            style="width: 100%">

                            {{#websiteData}}
                            {{#isGame}}
                            {{#big}}
                            {{/big}}
                            {{^big}}
                            <div class="col-sm-2">
                                <div class="divider">
                                    <img src={{image0_string}} style="width:
                                        128px; height: 128px; display: block;">
                                    <button class="btn" value={{id}}>{{name}}</button>
                                </div>
                            </div>
                            {{/big}}
                            {{/isGame}}
                            {{/websiteData}}
                        </div>

                </div>

                <!--Game Dev Skills -->
                <div id="gameDevSkills" class="main">
                    {{#paragraphData}}
                    <ul class="list-group">
                        <li class="list-group-item list-group-item-dark
                            list-group-item-action colour blue"
                            data-toggle="collapse"
                            data-target="#gameDevSkillList"><b>Game
                                Programmer Skills</b></li>
                        {{#gameDevSkills}}
                        <div id="gameDevSkillList" class="collapse">

                            <li class="list-group-item
                                list-group-item-dark
                                colour">{{{name}}}</li>
                        </div>

                        {{/gameDevSkills}}
                    </ul>
                    {{/paragraphData}}

                </div>

                {{/gameProjectsEnabled}}



                <!-- Project Information, Images and Links -->

                {{#websiteData}}
                <div id={{id}} style="display: none;">


                    <!--Show row of images, videos and a description at the end. !-->
                    <div class="project">

                        <div class="container">

                            <div class="row"
                                style="border-bottom-left-radius: 0;
                                border-bottom-right-radius: 0;">

                                <h2 style="padding: 1px; padding-top:0px;
                                margin-bottom:none; width:100%;">{{name}}
                                <button class="close" value={{id}}>X</button>
                                </h2>


                                <div class="col-lg">
                                    <div class="container">

                                        <div class="col-md">

                                            {{#video_string}}

                                            <div class="divider">
                                                <iframe width="320" height="256"
                                                    src={{video_string}}
                                                    frameborder="0"
                                                    allow="accelerometer;
                                                    autoplay; encrypted-media;
                                                    gyroscope;
                                                    picture-in-picture"
                                                    allowfullscreen></iframe>
                                            </div>

                                            {{/video_string}}

                                            <div class="divider">
                                                <img src={{image0_string}}
                                                    class="previewImage">
                                            </div>

                                        </div>


                                        <br>
                                        <br>
                                        <div class="col" style="text-align:
                                            center;">
                                            <!--<button class="prevPreview"
                                                id={{id}}_prev>
                                            </button> -->
                                            <span>
                                                <button class="changePreview"
                                                    value=0 id={{id}}_preview>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=1>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=2>
                                                    <br><br><br>
                                                </button>
                                                <button class="changePreview"
                                                    value=3> <br><br><br>
                                                </button>
                                            </span>
                                            <!--<button class="nextPreview"
                                                id={{id}}_next>
                                            </button> -->
                                        </div>
                                        <br>
                                        <br>


                                    </div>
                                </div>

                                <div class="col-xl"> <!--Description and links-->
                                    <div class="container">

                                        <!--Description -->
                                        <div class="col-lg">
                                            <br>
                                            {{description0}}
                                            <br>
                                            <br>
                                            {{description1}}
                                            <br>
                                            <br>
                                        </div>

                                        <!--Link to website and/or repository -->
                                        <div class="col-lg">
                                            {{#url}}
                                            <br>
                                            <br>
                                            <a href={{url}} class="siteLink" style="padding-right:20%;"
                                                target="_blank"> Visit Site</a>
                                            <br>
                                            <br>
                                            {{/url}}
                                            {{#repoLink}}
                                            <a href={{repoLink}}
                                                class="repoLink" style="padding-right:5%;"
                                                target="_blank"> Source Code
                                            </a>
                                            {{/repoLink}}
                                            <br>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <div class="row" style="border-top-left-radius: 0;
                                border-top-right-radius: 0;">

                                <!-- Optional links go below -->
                                <div class="col-lg">


                                </div>

                                <!-- Framework icon -->
                                <div class="col-xl">

                                    <img src={{frameworkImage_string}}
                                        
                                        class="framework">

                                </div>

                            </div>
                        </div>

                        <div class="itemStrings" style="display: none;">
                            <p>{{video_string}}</p>
                            <p>{{image0_string}}</p>
                            <p>{{image1_string}}</p>
                            <p>{{image2_string}}</p>
                            <p>{{image3_string}}</p>
                        </div>
                    </div>
                </div>
                {{/websiteData}}

            </div>


            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}

            <br>
            <br>
            <br>
            <a id="contactMe"
                href="https://mail.google.com/mail/u/0/?view=cm&fs=1&tf=1&source=mailto&su=&to=ScottCooperDeveloper@gmail.com"
                style="content-align: center; font-size:24pt;">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi
                    bi-envelope-fill" fill="currentColor"
                    xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M.05 3.555A2 2 0 0 1 2 2h12a2 2
                        0 0 1 1.95 1.555L8 8.414.05 3.555zM0
                        4.697v7.104l5.803-3.558L0 4.697zM6.761 8.83l-6.57
                        4.027A2 2 0 0 0 2 14h12a2 2 0 0 0
                        1.808-1.144l-6.57-4.027L8
                        9.586l-1.239-.757zm3.436-.586L16 11.801V4.697l-5.803
                        3.546z" />
                    </svg> Contact Me</a>
            </body>

        </html>