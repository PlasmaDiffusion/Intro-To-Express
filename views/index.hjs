<html>

    <head>
        <link
            href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
            crossorigin="anonymous">
        <link rel="stylesheet" type="text/css"
            href="/css/bootstrap/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="/css/style.css">
        <script type="module" src="js/menuEvents.js"></script>
        <script
            src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script
            src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
        <script
            src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    </head>

    <body>
        <div class="container">

            <div id="Opening" class="jumbotron">

                <h2 style="border-top-left-radius: 0;
                    border-top-right-radius: 0;" class="display-4">Scott Cooper
                    <br>
                    {{#paragraphData}}
                    <!-- Web dev title -->
                    {{#websiteProjectsEnabled}}
                    <a href="#websites_Scroll">{{webDevTitle}}</a>
                    {{/websiteProjectsEnabled}}
                    <!-- Slash if showing both -->
                    {{#bothProjectTypesEnabled}}
                    /
                    {{/bothProjectTypesEnabled}}
                    <!-- Game dev title -->
                    {{#gameProjectsEnabled}}<a
                        href="#games_Scroll">{{gameDevTitle}}</a>
                    {{/gameProjectsEnabled}}


                    {{/paragraphData}}
                </h2>
                {{#paragraphData}}
                <p class="lead">{{introParagraph1}}</p>


                <p class="lead">{{introParagraph2}}</p>
                {{/paragraphData}}
            </div>

            {{#websiteProjectsEnabled}}
            <div id="websites_Scroll">
            </div>

            <div class="BigSpace">
            </div>




            <!-- Website Menu -->


            <div id="websites">


                <h2>Website Projects</h2>
                {{#paragraphData}}
                <p style="text-align: center;" class="lead">{{webDevParagraph}}</p>
                {{/paragraphData}}
                <table>

                    <tr>

                        {{#websiteData}}
                        {{#isGame}}
                        {{/isGame}}
                        {{^isGame}}
                        <td>
                            <div class="divider">
                                <img src={{image0_string}} style="width: 128px;
                                    height: 128px; display: block;">
                                <button class="btn" value={{id}}>{{name}}</button>
                            </div>
                        </td>
                        {{/isGame}}
                        {{/websiteData}}
                    </tr>

                </table>
            </div>

            <!-- Website Skills -->
            <div id="webDevSkills" class="main">
                {{#paragraphData}}
                <ul class="list-group">
                    <li class="list-group-item list-group-item-dark
                        list-group-item-action colour blue"
                        data-toggle="collapse"
                        data-target="#webDevSkillList"><b>Web
                            Developer Skills</b></li>
                    {{#webDevSkills}}
                    <div id="webDevSkillList" class="collapse">

                        <li class="list-group-item
                            list-group-item-dark
                            colour">{{{name}}}</li>
                    </div>

                    {{/webDevSkills}}
                </ul>
                {{/paragraphData}}

            </div>

            <br>



            <div class="BigSpace">
            </div>

            {{/websiteProjectsEnabled}}

            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}


            <div id="games_Scroll">


                {{#gameProjectsEnabled}}
                <!-- Game Menu -->

                <div id="games">
                    <h2>Game Projects</h2>
                    {{#paragraphData}}
                    <p style="text-align: center;" class="lead">{{gameDevParagraph}}</p>
                    {{/paragraphData}}
                    <table>

                        <tr>

                            {{#websiteData}}
                            {{#isGame}}
                            <td>
                                <div class="divider">
                                    <img src={{image0_string}} style="width:
                                        128px; height: 128px; display: block;">
                                    <button class="btn" value={{id}}>{{name}}</button>
                                </div>
                            </td>
                            {{/isGame}}
                            {{/websiteData}}
                        </tr>

                    </table>
                </div>

                <!--Game Dev Skills -->
                <div id="gameDevSkills" class="main">
                    {{#paragraphData}}
                    <ul class="list-group">
                        <li class="list-group-item list-group-item-dark
                            list-group-item-action colour blue"
                            data-toggle="collapse"
                            data-target="#gameDevSkillList"><b>Game
                                Programmer Skills</b></li>
                        {{#gameDevSkills}}
                        <div id="gameDevSkillList" class="collapse">

                            <li class="list-group-item
                                list-group-item-dark
                                colour">{{name}}</li>
                        </div>

                        {{/gameDevSkills}}
                    </ul>
                    {{/paragraphData}}

                </div>

                {{/gameProjectsEnabled}}

                <!-- Project Information, Images and Links -->

                {{#websiteData}}
                <div id={{id}} style="display: none;">


                    <!--Show row of images, videos and a description at the end. !-->
                    <div class="divider" style="margin-left: 10%; margin-right:
                        50%;">

                        <h2 style="padding: 1px; padding-bottom: 10px;
                            margin-bottom:none; width:141.5%;">{{name}}
                            <button class="close" value={{id}}>X</button>
                        </h2>
                        <div class="container">

                            <div class="row"
                                style="border-bottom-left-radius: 0;
                                border-bottom-right-radius: 0;">

                                <div class="col-lg">
                                    <div class="container">

                                        <div class="col-md">

                                            {{#video_string}}

                                            <div class="divider">
                                                <iframe width="320" height="256"
                                                    src={{video_string}}
                                                    frameborder="0"
                                                    allow="accelerometer;
                                                    autoplay; encrypted-media;
                                                    gyroscope;
                                                    picture-in-picture"
                                                    allowfullscreen></iframe>
                                            </div>

                                            {{/video_string}}

                                            <div class="divider">
                                                <img src={{image0_string}}
                                                    class="previewImage">
                                            </div>

                                        </div>


                                        <br>
                                        <br>
                                        <div class="col" style="text-align:
                                            center;">
                                            <button class="prevPreview"
                                                id={{id}}_prev> &#8249;
                                            </button>
                                            <span>
                                                <button class="changePreview"
                                                    value=0 id={{id}}_preview>
                                                    &nbsp; </button>
                                                <button class="changePreview"
                                                    value=1> &nbsp; </button>
                                                <button class="changePreview"
                                                    value=2> &nbsp; </button>
                                                <button class="changePreview"
                                                    value=3> &nbsp; </button>
                                            </span>
                                            <button class="nextPreview"
                                                id={{id}}_next> &#8250;
                                            </button>
                                        </div>
                                        <br>
                                        <br>


                                    </div>
                                </div>

                                <div class="col-xl"> <!--Description and links-->
                                    <div class="container">

                                        <!--Description -->
                                        <div class="col-lg">
                                            <br>
                                            {{description0}}
                                            <br>
                                            <br>
                                            {{description1}}
                                            <br>
                                            <br>
                                        </div>

                                        <!--Link to website and/or repository -->
                                        <div class="col-lg">
                                            {{#url}}
                                            <br>
                                            <br>
                                            <a href={{url}} class="siteLink"
                                                target="_blank"> Try it out!</a>
                                            {{/url}}
                                            {{#repoLink}}
                                            <a href={{repoLink}}
                                                class="repoLink"
                                                target="_blank"> Source Code
                                            </a>
                                            {{/repoLink}}
                                            <br>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <div class="row" style="border-top-left-radius: 0;
                                border-top-right-radius: 0;">

                                <!-- Optional links go below -->
                                <div class="col-lg">


                                </div>

                                <!-- Framework icon -->
                                <div class="col-xl">

                                    <img src={{frameworkImage_string}}
                                        style="width:64;height:64px;"
                                        class="framework">

                                </div>

                            </div>
                        </div>

                        <div class="itemStrings" style="display: none;">
                            <p>{{video_string}}</p>
                            <p>{{image0_string}}</p>
                            <p>{{image1_string}}</p>
                            <p>{{image2_string}}</p>
                            <p>{{image3_string}}</p>
                        </div>
                    </div>
                </div>
                {{/websiteData}}

            </div>


            {{#gameProjectsEnabled}}
            <div class="BigSpace">
            </div>
            {{/gameProjectsEnabled}}

            <br>
            <br>
            <br>
            <a id="contactMe"
                href="https://mail.google.com/mail/u/0/?view=cm&fs=1&tf=1&source=mailto&su=&to=ScottHRCooper@gmail.com"
                style="content-align: center; font-size:24pt;">Contact Me</a>
        </body>

    </html>